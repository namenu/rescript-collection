{
  "name": "rescript-vector",
  "version": "0.4.0",
  "description": "Persistent Vector implemenetation in ReScript",
  "license": "MIT",
  "author": {
    "name": "Hyunwoo Nam"
  },
  "keywords": [
    "rescript",
    "data-structure",
    "immutable",
    "vector"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "imports": {
    "@rescript/std/lib/es6/": {
      "import": "@rescript/std/lib/es6/",
      "require": "@rescript/std/lib/js/"
    }
  },
  "scripts": {
    "res:build": "rescript build -with-deps",
    "res:watch": "rescript build -w",
    "res:clean": "rescript clean",
    "bundle": "nanobundle build --clean",
    "prepack": "yarn res:build && yarn bundle",
    "test": "pta 'tests/*_test.mjs'",
    "test:watch": "onchange --initial '{tests,src}/*.mjs' -- yarn test",
    "test:coverage": "c8 -r text -r lcov yarn test"
  },
  "files": [
    "dist",
    "src/*.res",
    "src/*.resi",
    "bsconfig.json"
  ],
  "dependencies": {
    "@rescript/std": "^10.1.0"
  },
  "devDependencies": {
    "@dusty-phillips/rescript-zora": "^4.0.0",
    "browserslist": "^4.21.4",
    "c8": "^7.12.0",
    "nanobundle": "^1.3.6",
    "onchange": "^7.1.0",
    "pta": "^1.2.0",
    "rescript": "^10.1.0",
    "rescript-fast-check": "^1.1.1",
    "typescript": "^4.9.4"
  }
}
